<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- NON-TRANSLATABLE VALUES - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->


    <!-- This file should NOT be translated. MathDoku will probably not work correctly in case values are translated or overwritten. -->


    <!-- Name of application. This may NOT be translated. -->
    <string name="application_name">MathDoku</string>

    <!-- The possible values of the "themes" option. This option controls which theme is used for displaying. -->
    <string-array name="themeTexts">
        <item>@string/theme_light</item>
        <item>@string/theme_dark</item>
    </string-array>

    <!-- Identifiers associated with element themeTexts. This element may not be translated. -->
    <string-array name="themeValues">
        <item>theme_light</item>
        <item>theme_dark</item>
    </string-array>

    <!-- The possible values of the "maxGamesElapsedTimeChart" preference. -->
    <string-array name="maxGamesElapsedTimeChartValues">
        <item>50</item>
        <item>100</item>
        <item>150</item>
        <item>250</item>
    </string-array>

</resources>