    <!-- CONTEXT MENU IN PUZZLE - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->


    <!-- The context menu item that allows the user to reveal a cell. -->
    <string name="context_menu_reveal_cell">Onthul deze cel</string>

    <!-- The context menu item that allows the user to reveal the hidden operator of a cage. This item is only available in case a puzzle with hidden operators is shown and the operator has not yet been revealed. Note that a single cell cage does not have an operator. -->
    <string name="context_menu_reveal_operator">Toon groep teken</string>

