    <string name="dialog_survey_user_name_hint">Ihr Name</string>
    <string name="dialog_survey_user_email_hint">Ihre E-Mail</string>
    <string name="dialog_survey_positive_button">Logfile senden</string>

    <!-- Title of dialog used to choose the appropriate action for sending the usage log. -->
